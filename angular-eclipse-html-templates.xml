<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<templates>
	<template autoinsert="true" context="html_all" deleted="false"
		description="angular date formatter" enabled="true" name="date">{{
		${expressionToFormat} | date[:${dateFormat}] }}</template>
	<template autoinsert="true" context="html_all" deleted="false"
		description="angular filter" enabled="true" name="filter">{{
		${expressionToFilter} | filter:expression }}</template>
	<template autoinsert="true" context="html_all" deleted="false"
		description="angular - json formatter" enabled="true" name="json">{{
		${expressionToFormat} | json }}</template>
	<template autoinsert="true" context="html_all" deleted="false"
		description="angular filter - limit array to certain elements"
		enabled="true" name="limitTo">{{ ${arrayToLimit} | limitTo:${limit} }}
	</template>
	<template autoinsert="true" context="html_all" deleted="false"
		description="angular - expression to lowercase" enabled="true" name="lowercase">{{
		${expressionToLowercase} | lowercase }}</template>
	<template autoinsert="true" context="html_all" deleted="false"
		description="angular - format a number as text" enabled="true" name="number">{{
		${numberExpressionToFormat} | number[:${fractionSize}] }}</template>
	<template autoinsert="true" context="html_all" deleted="false"
		description="angular order-by filter array" enabled="true" name="orderBy">|
		orderBy:${predicateVariable}:${reverseVariable}</template>
	<template autoinsert="true" context="html_all" deleted="false"
		description="angular uppercase" enabled="true" name="uppercase">{{
		${expressionToUppercase} | uppercase }}</template>
	<template autoinsert="true" context="html_tag" deleted="false"
		description="comment" enabled="true"
		id="org.eclipse.wst.html.ui.templates.comment" name="comment">&lt;!-- ${cursor}
		--&gt;</template>
	<template autoinsert="true" context="html_all" deleted="false"
		description="angular currency formatting" enabled="true" name="currency">{{
		${expressionToFormat} | currency[:${currencySymbol}] }}</template>
	<template autoinsert="true" context="html_attribute" deleted="false"
		description="angular app" enabled="true" name="ng-app">ng-app="${moduleName}"
	</template>
	<template autoinsert="true" context="html_attribute" deleted="false"
		description="The ngBind attribute tells Angular to replace the text content of the specified HTML element with the value of a given expression, and to update the text content when the value of that expression changes."
		enabled="true" name="ng-bind">ng-bind="${expression}"</template>
	<template autoinsert="true" context="html_attribute" deleted="false"
		description="Creates a binding that will innerHTML the result of evaluating the expression into the current element. The innerHTML-ed content will not be sanitized! You should use this directive only if ngBindHtml directive is too restrictive and when you absolutely trust the source of the content you are binding to."
		enabled="true" name="ng-bind-html-unsafe">ng-bind-html-unsafe="${expression}"</template>
	<template autoinsert="true" context="html_attribute" deleted="false"
		description="angular ng-change" enabled="true" name="ng-change">ng-change="${expression}"
	</template>
	<template autoinsert="true" context="html_attribute" deleted="false"
		description="angular checked" enabled="true" name="ng-checked">ng-checked="${expression}"
	</template>
	<template autoinsert="true" context="html_attribute" deleted="false"
		description="angular class" enabled="true" name="ng-class">ng-class="${expression}"
	</template>
	<template autoinsert="true" context="html_attribute" deleted="false"
		description="angular - set the class on even rows" enabled="true"
		name="ng-class-even">ng-class-even="${expression}"</template>
	<template autoinsert="true" context="html_attribute" deleted="false"
		description="angular class odd" enabled="true" name="ng-class-odd">ng-class-odd="${expression}"
	</template>
	<template autoinsert="true" context="html_attribute" deleted="false"
		description="angular-click" enabled="true" name="ng-click">ng-click="${expression}"
	</template>
	<template autoinsert="true" context="html_attribute" deleted="false"
		description="angular prevent template from being displayed while loading"
		enabled="true" name="ng-cloak">ng-cloak</template>
	<template autoinsert="true" context="html_attribute" deleted="false"
		description="angular controller" enabled="true" name="ng-controller">ng-controller="${expression}"
	</template>
	<template autoinsert="true" context="html_attribute" deleted="false"
		description="angular cross-site security policy" enabled="true" name="ng-csp">ng-csp
	</template>
	<template autoinsert="true" context="html_attribute" deleted="false"
		description="angular double click" enabled="true" name="ng-dblclick">ng-dblclick="${expression}"
	</template>
	<template autoinsert="true" context="html_attribute" deleted="false"
		description="angular disabled" enabled="true" name="ng-disabled">ng-disabled="${expression}"
	</template>
	<template autoinsert="true" context="html_attribute" deleted="false"
		description="angular form" enabled="true" name="ng-form">ng-form
		name="${expression}"</template>
	<template autoinsert="true" context="html_attribute" deleted="false"
		description="angular-hide" enabled="true" name="ng-hide">ng-hide="${expression}"
	</template>
	<template autoinsert="true" context="html_attribute" deleted="false"
		description="angular href" enabled="true" name="ng-href">ng-href="${expression}"
	</template>
	<template autoinsert="true" context="html_attribute" deleted="false"
		description="angular- include   optional onload=&quot;&quot; and autoscroll=&quot;&quot;"
		enabled="true" name="ng-include">ng-include="${expression}"</template>
	<template autoinsert="true" context="html_attribute" deleted="false"
		description="angular init - tasks to load before execution mode"
		enabled="true" name="ng-init">ng-init="${expression}"</template>
	<template autoinsert="true" context="html_attribute" deleted="false"
		description="angular - converts comma separated strings into an array"
		enabled="true" name="ng-list">ng-list="${expression}"</template>
	<template autoinsert="true" context="html_attribute" deleted="false"
		description="angular - two way data binding" enabled="true" name="ng-model">ng-model="${expression}"
	</template>
	<template autoinsert="true" context="html_attribute" deleted="false"
		description="angular mousedown event" enabled="true" name="ng-mousedown">ng-mousedown="${expression}"
	</template>
	<template autoinsert="true" context="html_attribute" deleted="false"
		description="" enabled="true" name="ng-mouseenter">ng-mouseenter="${expression}"
	</template>
	<template autoinsert="true" context="html_attribute" deleted="false"
		description="angular mouse leave event" enabled="true" name="ng-mouseleave">ng-mouseleave="${expression}"
	</template>
	<template autoinsert="true" context="html_attribute" deleted="false"
		description="angular mouse move event" enabled="true" name="ng-mousemove">ng-mousemove="${expression}"
	</template>
	<template autoinsert="true" context="html_attribute" deleted="false"
		description="angular mouse over event" enabled="true" name="ng-mouseover">ng-mouseover="${expression}"
	</template>
	<template autoinsert="true" context="html_attribute" deleted="false"
		description="angular mouse up event" enabled="true" name="ng-mouseup">ng-mouseup="${expression}"
	</template>
	<template autoinsert="true" context="html_attribute" deleted="false"
		description="angular multiple" enabled="true" name="ng-multiple">ng-multiple="${expression}"
	</template>
	<template autoinsert="true" context="html_attribute" deleted="false"
		description="angular - non-bindable chunk of code" enabled="true"
		name="ng-non-bindable">ng-non-bindable="${expression}"</template>
	<template autoinsert="true" context="html_tag" deleted="false"
		description="angular pluralize" enabled="true" name="ng-pluralize">&lt;ng-pluralize
		count="${expression}"
		when="${string}"
		[offset="${number}"]&gt;
		&lt;/ng-pluralize&gt;</template>
	<template autoinsert="true" context="html_attribute" deleted="false"
		description="angular make tag read only" enabled="true" name="ng-readonly">ng-readonly="${expression}"
	</template>
	<template autoinsert="true" context="html_attribute" deleted="false"
		description="angular repeat" enabled="true" name="ng-repeat">ng-repeat="${var} in
		${vars}"</template>
	<template autoinsert="true" context="html_tag" deleted="false"
		description="angular script for script-cache  (include, templates)"
		enabled="true" name="ng-script">&lt;script
		type="text/ng-template"&gt;
		&lt;/script&gt;</template>
	<template autoinsert="true" context="html_tag" deleted="false"
		description="angular select" enabled="true" name="ng-select">&lt;select
		name="{string}"
		[required]
		[ngRequired="{string}"]
		[ngOptions="{comprehension_expression}"]&gt;
		&lt;/select&gt;</template>
	<template autoinsert="true" context="html_attribute" deleted="false"
		description="angular - selected (use in option)" enabled="true" name="ng-selected">ng-selected="${expression}"
	</template>
	<template autoinsert="true" context="html_attribute" deleted="false"
		description="angular show this field" enabled="true" name="ng-show">ng-show="${expression}"
	</template>
	<template autoinsert="true" context="html_attribute" deleted="false"
		description="angular - source for image or other link source" enabled="true"
		name="ng-src">ng-src="${expression}"</template>
	<template autoinsert="true" context="html_attribute" deleted="false"
		description="angular style" enabled="true" name="ng-style">ng-style="${expression}"
	</template>
	<template autoinsert="true" context="html_attribute" deleted="false"
		description="angular submit" enabled="true" name="ng-submit">ng-submit="${expression}"
	</template>
	<template autoinsert="true" context="html_tag" deleted="false"
		description="angular switch statement" enabled="true" name="ng-switch">&lt;ng-switch
		on="{${cursor}*}"&gt;
		&lt;/ng-switch&gt;</template>
	<template autoinsert="true" context="html_tag" deleted="false"
		description="angular text area" enabled="true" name="ng-textarea">&lt;textarea
		ngModel="{string}"
		[name="{string}"]
		[required]
		[ngRequired="{string}"]
		[ngMinlength="{number}"]
		[ngMaxlength="{number}"]
		[ngPattern="{string}"]
		[ngChange="{string}"]&gt;
		&lt;/textarea&gt;</template>
	<template autoinsert="true" context="html_attribute" deleted="false"
		description="angular  insert transclude dom here" enabled="true"
		name="ng-transclude">ng-transclude</template>
	<template autoinsert="true" context="html_attribute" deleted="false"
		description="angular view" enabled="true" name="ng-view">ng-view ${cursor}
	</template>
	<template autoinsert="true" context="html_all" deleted="false"
		description="A binding in AngularJS" enabled="true" name="ngb">{{${binding}}}${cursor}
	</template>
	<template autoinsert="true" context="html_all" deleted="false"
		description="Simple way of bootstraping angular app for prototyping purposes"
		enabled="true" name="ngindex">&lt;html&gt;
		&lt;head&gt;
		&lt;script
		src="https://ajax.googleapis.com/ajax/libs/angularjs/${version}/angular.js"&gt;&lt;/script&gt;
		&lt;/head&gt;
		&lt;body ng-app&gt;
		${cursor}
		&lt;/body&gt;
		&lt;/html&gt;</template>
	<template autoinsert="true" context="html_all" deleted="false"
		description="Script tag importing base AngularJS file from CDN"
		enabled="true" name="ngsa">&lt;script type="text/javascript"
		src="http://ajax.googleapis.com/ajax/libs/angularjs/${version}/angular${suffix}.js"&gt;&lt;/script&gt;
		${cursor}
	</template>
	<template autoinsert="true" context="html_all" deleted="false"
		description="A script tag holding Angular's template" enabled="true"
		name="ngst">&lt;script type="text/ng-template" id="${id}"&gt;
		${cursor}
		&lt;/script&gt;</template>
</templates>